<launch>
  <arg name="gzname" default="gazebo"/>
  <arg name="WORLD" default="STARO.world"/>
  <arg name="PAUSED" default="false"/>
  <arg name="SYNCHRONIZED" default="false" />

  <include file="$(find hrpsys_gazebo_tutorials)/launch/gazebo_robot_no_controllers.launch">
    <arg name="ROBOT_TYPE" value="STARO" />
    <arg name="USE_INSTANCE_NAME" value="true" />
    <arg name="gzname" value="$(arg gzname)" />
    <arg name="WORLD" value="$(arg WORLD)" />
    <arg name="PAUSED" value="$(arg PAUSED)"/>
    <!-- <arg name="HRPSYS_GAZEBO_CONFIG" value="$(find hrpsys_gazebo_tutorials)/config/STARO.yaml" /> -->
    <!-- <arg name="ROBOT_MODEL" value="$(find hrpsys_gazebo_tutorials)/robot_models/STARO/STARO.urdf" /> -->
    <arg name="SYNCHRONIZED" value="$(arg SYNCHRONIZED)" />
  </include>
</launch>
